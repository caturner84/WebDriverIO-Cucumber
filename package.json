{
  "name": "webdriverio-cucumber",
  "version": "1.0.0",
  "description": "This is an example WebDriverIO + Cucumber framework",
  "scripts": {
    "conf-example": "./node_modules/.bin/wdio wdio.example.conf.js",
    "es-lint": "./node_modules/.bin/eslint --ignore-path .gitignore --ext js,jsx,snap .",
    "gherkin-lint": "gherkin-lint > ./reports/gherkin-lint-results/results.txt 2>&1",
    "test-local-example": "npm run gherkin-lint && npm run conf-example -- --runOnSauce=false --suite=exampleTests"
  },
  "keywords": [
    "automation-practise",
    "testing",
    "webdriverio",
    "javascript",
    "gherkin",
    "cucumber"
  ],
  "author": "Chris Turner",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@types/cucumber": "^6.0.1",
    "@wdio/cli": "^6.6.7",
    "@wdio/cucumber-framework": "^6.6.6",
    "@wdio/devtools-service": "^6.6.6",
    "@wdio/local-runner": "^6.9.0",
    "@wdio/sauce-service": "^6.6.7",
    "@wdio/selenium-standalone-service": "^6.9.0",
    "@wdio/sync": "^6.6.7",
    "chai": "^4.2.0",
    "cucumber": "^6.0.5",
    "eslint": "^5.16.0",
    "eslint-config-amex": "^10.1.0",
    "fibers": "^3.1.1",
    "fs-extra": "^8.1.0",
    "gherkin-lint": "^3.4.0",
    "multiple-cucumber-html-reporter": "1.18.0",
    "oracledb": "^3.1.2",
    "rimraf": "^3.0.2",
    "wdio-cucumberjs-json-reporter": "2.0.2",
    "yargs": "^16.1.0"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/caturner84/WebDriverIO-Cucumber.git"
  }
}
